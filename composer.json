{
  "name": "yuncms/yuncms-attention",
  "description": "The attention module for yuncms.",
  "keywords": [
    "attention",
    "yii2",
    "yii2-module"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Xu TL",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yuncms/yuncms-user": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\attention\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "id": "attention",
      "translate": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/attention/messages"
      },
      "migrationNamespace": "yuncms\\attention\\migrations",
      "backend": {
      },
      "frontend": {
        "class": "yuncms\\attention\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
